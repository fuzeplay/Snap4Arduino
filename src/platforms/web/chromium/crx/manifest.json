{
    "name": "Snap4Fuze connector",
    "description": "Connect Berkeley Snap! to Fuze boards",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
        "background": {
            "page": "background.html"
        }
    },
    "permissions": [
        "serial"
    ],
    "externally_connectable": {
        "matches": [
            "http://dev.arduino.org/*",
            "http://arduino.org/*",
            "http://46.140.176.36/*",
            "http://localhost:8080/*",
            "http://localhost:3000/*",
            "http://localhost:5000/*",
            "http://localhost/*",
            "http://snap4arduino.local/*",
            "http://snap4arduino.org/*",
            "http://snap4arduino.rocks/*",
            "https://snap4arduino.rocks/*",
            "https://snap4fuze.firebaseapp.com/",
            "https://snap4fuze.com/"
        ],
        "accepts_tls_channel_id": false
    },
    "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}
